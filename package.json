{
  "name": "express-ejs-blog-webapp",
  "version": "1.0.0",
  "description": "A simple blog webapp built using express, ejs, typescript",
  "main": "server.ts",
  "scripts": {
    "tst": "tsc",
    "start": "npm run dev",
    "dev": "ts-node-dev --respawn --transpileOnly  ./src/server.ts",
    "dev:debug": "ts-node-dev --transpileOnly --respawn --debug ./src/server.ts",
    "prod": "tsc && node ./dist/index.js",
    "build": "tsc -p",
    "health-check": "ts-node-dev ./src/utils/healthCheck.ts"
  },
  "author": "Lakshya Dev",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^4.0.0",
    "ejs": "^3.1.3",
    "express": "^4.18.2",
    "lodash": "^4.17.15",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.9",
    "@types/request": "^2.48.5",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  },
  "type": "module"
}
